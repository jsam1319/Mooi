<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration   PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
                          "http://mybatis.org/dtd/mybatis-3-config.dtd">

<!-- MyBatis XML 설정 파일(MyBatis설정의 중앙 허브 역할) -->
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"></setting>
	</settings>


	<typeAliases>
		<typeAlias alias="Member" type="kr.co.mooi.member.domain.Member"/>
		<typeAlias alias="Product" type="kr.co.mooi.product.domain.Product"/>
		<typeAlias alias="Keyword" type="kr.co.mooi.keyword.domain.Keyword"/>
		<typeAlias alias="PageBuilder" type="kr.co.mooi.product.domain.PageBuilder"/>
		<typeAlias alias="Cart" type="kr.co.mooi.cart.domain.Cart"/>
		<typeAlias alias="Orders" type="kr.co.mooi.orders.domain.Orders"/>
		
	</typeAliases>
</configuration>